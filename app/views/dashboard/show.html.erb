<div class="dashboard-header">
  <h2>Dashboard</h2>


<div class="card card-no-lines" style="width=100%">

  <div class="card-body" >
        <%= image_tag current_user.image.url, class:'', size:"250x250" %>
  </div>
  <div class="card-body">
    <h5 class="card-title">Hi, <%= current_user.f_name%></h5>
    <p class="card-text"></p>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">First Name: <%= current_user.f_name%></li>
    <li class="list-group-item">Last Name: <%= current_user.l_name%> </li>
    <li class="list-group-item">Current Email Address: <%= current_user.email %></li>
  </ul>
  <div class="text-center">
    <button id="edit-user-profile-button" class="primary-btn btn-sm card-button "><span ><%= link_to "Edit", edit_user_path(current_user) %></span></button>
  </div>
  <br>
  <div class="card-deck ">
  <!-- <div class="card"> -->
        <div class = 'text-center dash-btn-padding'>
          <h4 class=""> Check Your <br>DATER Profile</h4>
          <button id="dater-profile-button" class="primary-btn btn-sm card-button"><span ><%= link_to "Dater", edit_dater_path(current_dater) %></span></button>
        </div>

        <div class = 'text-center dash-btn-padding'>
          <h4 class="">Visit Your <br>BACKER Profile</h4>
            <button id="backer-profile-button" class="primary-btn btn-sm card-button"><span ><%= link_to "Backer", backer_path(current_backer) %></span></button>
        </div>
    </div>
  </div>

  </div>
</div>
